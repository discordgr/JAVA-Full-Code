public class Friges extends HouseholdAppliances
{
	protected String FrigeType;
	protected float FrigeSpaceUp;
	protected float FrigeSpaceDown;
	
	public Friges(int code, String name, String year, String manufacturer, float price, String energysave, String FrigeType, float FrigeSpaceUp, float FrigeSpaceDown)
	{
		super(code, name, year, manufacturer, price, energysave );
		this.FrigeSpaceUp = FrigeSpaceUp;
		this.FrigeSpaceDown = FrigeSpaceDown;
	}	
		
		// All getters
		public String getFrigeType()
		{
			return FrigeType;
		}
		
		public float getFrigeSpaceUp()
		{
			return FrigeSpaceUp;
		}
		
		public float getFrigeSpaceDown()
		{
			return FrigeSpaceDown;
		}
		
		public void setFrigeType(String FrigeType)
		{
			this.FrigeType = FrigeType;
		}
		
		public void setFrigeSpaceUp(float FrigeSpaceUp)
		{
			this.FrigeSpaceUp = FrigeSpaceUp;
		}
		
	
		public void setFrigeSpaceDown(float FrigeSpaceDown)
		{
			this.FrigeSpaceDown = FrigeSpaceDown;
		}
		
		public String toString()
		{
			return "\nFridge refrigeration Space:  " + this.FrigeSpaceUp + "\nFridge Main Space: " + this.FrigeSpaceDown;
		}



}
